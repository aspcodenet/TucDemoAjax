@page
@model TucDemoAjax.Pages.GamesModel
@{
}


<table class="table">
    <thead>
        <tr>
            <th scope="col">Date</th>
            <th scope="col">Teams</th>
            <th scope="col">Result</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.GameListItems)
        {

            <tr>
                <td>@item.Datum</td>
                <td>@item.Hometeam - @item.Awayteam</td>
                <td>@item.HomeScores - @item.AwayScores</td>
            </tr>
        }
    </tbody>
</table>


